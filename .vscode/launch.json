{
"version": "0.2.0",
"configurations": [
    {
        "name": "Debug chrome",
        "type": "chrome",
        "request": "launch",
        "preLaunchTask": "dev",
        "sourceMaps": true
    },
    {
        "name": "Debug node",
        "type": "node",
        "request": "launch",
        "program": "${workspaceRoot}/server/index.js",
        "stopOnEntry": false,
        "args": [],
        "cwd": "${workspaceRoot}",
        "runtimeExecutable": null,
        "runtimeArgs": [
            "--nolazy"
        ],
        "env": {
            "NODE_ENV": "dev"
        },
        "externalConsole": false,
        "preLaunchTask": "",
        "sourceMaps": true,
        "outDir": "${workspaceRoot}/server/assets/bin"
    }
]
}